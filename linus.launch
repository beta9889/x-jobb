<launch>
  <env name="GAZEBO_PLUGIN_PATH" value="$(find gazebo_plugins)"/>
  <env name="GAZEBO_MODEL_PATH" value="$(find turtlebot3_gazebo)/models"/>
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="move_forward_only" default="false"/>


  <include file="$(find turtlebot3_gazebo)/launch/turtlebot3_house.launch"/>

  <include file="$(find turtlebot3_slam)/launch/turtlebot3_slam.launch">
    <arg name="slam_methods" value="gmapping" />
    <arg name="configuration_basename" value="turtlebot3_lds_2d_gazebo.lua" />
  </include>

    <!-- move_base -->
  <include file="$(find turtlebot3_navigation)/launch/new_move_base.launch">
    <arg name="model" value="$(arg model)" />
    <arg name="move_forward_only" value="$(arg move_forward_only)"/>
    <arg name="use_quadratic" value="true"/>
    <arg name="use_dijkstra" value="true"/>
    <arg name="use_grid_path" value="false"/>
    <arg name="neutral_cost" value="20"/>
    <arg name="allow_unknown" value="true"/>
  </include>
  
  <node pkg="experiment_node" type="experiment_node" name="experiment_node" output="screen">
    <param name="algorithm" type="string" value="dijkstra" />
    <!-- <param name="goal_x" type="double" value="-0.02"/>
    <param name="goal_y" type="double" value="-0.52"/> -->
    <param name="world" type="string" value="house"/>
    <param name="env_type" type="string" value="static"/>
    <param name="map_type" type="string" value="mapless"/>
  </node>

</launch>